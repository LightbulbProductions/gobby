<div class='actions col-md-4 col-md-offset-2'>
<%= form_for @project do |f| %>
<div class='form-group'>
<%= f.label :title %>
<%= f.text_field :title, class: 'form-control' %>
</div>
<div class='form-group'>
	<%= f.label :description %>
	<%= f.text_field :description, class: 'form-control' %>
</div>
<div class='form-group'>
	<%= f.submit "save", class: 'btn btn-success'%>
</div>
<% end %>
</div>